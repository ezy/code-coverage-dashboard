{
    "env": {
        "node": true,
        "browser": true,
        "es6": true
    },
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
        "no-console": 0,
        "no-param-reassign": 0,
        "no-use-before-define": 0,
        "react/jsx-filename-extension": 0,
        "react/prop-types": 0,
        "react/destructuring-assignment": 0
    }
}
